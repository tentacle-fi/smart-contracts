{
  "name": "smart-contract-testing",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "test": "(TF_FOLDER=testing yarn _test)",
    "test_coverage": "(TF_FOLDER=testing yarn _coverage)",
    "test_compile": "(TF_FOLDER=testing yarn _compile)",
    "voting": "(TF_FOLDER=Voting yarn _test)",
    "voting_coverage": "(TF_FOLDER=Voting yarn _coverage)",
    "voting_compile": "(TF_FOLDER=Voting yarn _compile)",
    "_test": "cd $TF_FOLDER && npx truffle test ./test/test_1.js",
    "_compile": "cd $TF_FOLDER && npx truffle compile",
    "post_compile": "node tools.js $TF_FOLDER/build/contracts/$TF_FOLDER.json",
    "_coverage": "cd $TF_FOLDER && npx truffle run coverage --file=\"**/test/*.js\"",
    "post_coverage": "cp ./node_modules/nyc-dark/*.css $TF_FOLDER/coverage/",
    "lint": "prettier --write \"**/*.+(js|json|sol)\" --print-width 150"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@openzeppelin/contracts": "^4.4.2",
    "@openzeppelin/test-helpers": "^0.5.15",
    "nyc-dark": "^3.0.3",
    "prettier": "^2.5.1",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "solidity-coverage": "^0.7.19",
    "truffle": "^5.4.30"
  }
}
